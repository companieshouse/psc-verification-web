{
    "name": "psc-verification-web",
    "version": "1.0.0",
    "description": "A starter web application for Node.js with Typescript",
    "main": "dist/httpServer.js",
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10"
    },
    "scripts": {
        "prebuild": "rm -rf ./dist",
        "build:assets": "gulp build",
        "build:server": "tsc --project tsconfig-build.json && cp -r src/views dist/ && cp -r src/config dist/",
        "build": "tsc --project tsconfig-build.json && cp -r src/views dist/",
        "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
        "lint:fix": "eslint 'src/**/*.ts' 'test/**/*.ts' --fix",
        "coverage": "jest --coverage",
        "test": "jest",
        "test:coverage": "sh ./scripts/jest_coverage.sh",
        "start": "npm run build && node dist/httpServer.js",
        "start:watch": "nodemon",
        "chs-dev": "nodemon --legacy-watch",
        "sonarqube-base-branch": "sonar-scanner",
        "sonarqube-pull-request": "sonar-scanner -D sonar.pullrequest.base=main",
        "sonarqube": "branch=$(git rev-parse --symbolic-full-name --abbrev-ref HEAD); if [[ $branch == \"HEAD\" ]]; then echo $branch && npm run sonarqube-base-branch; else echo $branch && npm run sonarqube-pull-request; fi;",
        "prepare": "husky"
    },
    "author": "Moses Wejuli <mwejuli@companieshouse.gov.uk>",
    "license": "MIT",
    "dependencies": {
        "@companieshouse/api-sdk-node": "^2.0.271",
        "@companieshouse/ch-node-utils": "^2.1.2",
        "@companieshouse/node-session-handler": "^5.2.3",
        "@companieshouse/structured-logging-node": "^2.0.2",
        "@companieshouse/web-security-node": "^4.4.9",
        "@types/supertest": "^6.0.3",
        "axios": "^1.10.0",
        "cookie-parser": "1.4.7",
        "express": "^5.1.0",
        "govuk_frontend_toolkit": "^9.0.1",
        "govuk-elements-sass": "^3.1.3",
        "govuk-frontend": "^5.11.0",
        "http-errors": "^2.0.0",
        "ioredis": "5.6.1",
        "js-yaml": "^4.1.0",
        "luxon": "^3.6.1",
        "nunjucks": "3.2.4",
        "sonarqube-scanner": "^4.3.0",
        "tslib": "^2.8.1",
        "uuid": "^11.1.0",
        "yargs": "18.0.0"
    },
    "devDependencies": {
        "@types/cookie-parser": "^1.4.9",
        "@types/express": "^5.0.3",
        "@types/http-errors": "^2.0.5",
        "@types/jest": "^30.0.0",
        "@types/js-yaml": "^4.0.9",
        "@types/luxon": "^3.6.2",
        "@types/mocha": "^10.0.10",
        "@types/node": "^24.0.4",
        "@types/nunjucks": "3.2.6",
        "@types/uuid": "10.0.0",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "cheerio": "^1.1.0",
        "eslint": "^8.57.1",
        "eslint-config-standard": "14.1.1",
        "eslint-plugin-check-file": "^2.8.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "4.3.1",
        "eslint-plugin-standard": "4.1.0",
        "eslint-plugin-unused-imports": "^3.2.0",
        "gulp": "^5.0.1",
        "gulp-clean": "^0.4.0",
        "gulp-concat": "^2.6.1",
        "gulp-sass": "^6.0.1",
        "gulp-uglify": "^3.0.2",
        "husky": "^9.1.7",
        "jest": "^30.0.3",
        "mocha": "^11.7.1",
        "nock": "14.0.5",
        "node-mocks-http": "^1.17.2",
        "nodemon": "^3.1.10",
        "nyc": "^17.1.0",
        "sass": "1.89.2",
        "supertest": "^7.1.1",
        "ts-jest": "^29.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    },
    "overrides": {
        "chokidar": "3.5.3",
        "glob-parent": "6.0.2",
        "micromatch": "4.0.8"
    },
    "directories": {
        "test": "test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/companieshouse/node-review-web-starter-ts.git"
    },
    "keywords": [],
    "bugs": {
        "url": "https://github.com/companieshouse/node-review-web-starter-ts/issues"
    },
    "homepage": "https://github.com/companieshouse/node-review-web-starter-ts#readme"
}
